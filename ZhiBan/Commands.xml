<?xml version="1.0" encoding="utf-8" ?>
<KeyinTree xmlns="http://www.bentley.com/schemas/1.0/MicroStation/AddIn/KeyinTree.xsd">

    <RootKeyinTable ID="root">
        <Keyword SubtableRef="Commands" CommandWord="Output">
            <Options Required="true" />
        </Keyword>
    </RootKeyinTable>

    <SubKeyinTables>
        <KeyinTable ID="Commands">            
            <Keyword SubtableRef="FromCmd" CommandWord="Words">
                <Options Required="true" />
            </Keyword>
        </KeyinTable>

        <KeyinTable ID="FromCmd">
          <Keyword CommandWord="MainForm" />
          <Keyword CommandWord="LoginForm" />
          <Keyword CommandWord="AutoForm" />
        </KeyinTable>

    </SubKeyinTables>

    <KeyinHandlers>
        <KeyinHandler Keyin="Output Words MainForm"            Function="ZhiBan.Commands.MainForm"/>
        <KeyinHandler Keyin="Output Words LoginForm"            Function="ZhiBan.Commands.LoginForm"/>
        <KeyinHandler Keyin="Output Words AutoForm"            Function="ZhiBan.Commands.AutoForm"/>
    </KeyinHandlers>

</KeyinTree>

